<?xml version="1.0" encoding="utf-8"?>
<shape    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- 填充的顏色 -->
    <solid android:color="#0D0F7BFF" />
    <!-- 設置按鈕的四個角爲弧形 -->
    <!-- android:radius 弧形的半徑 -->
    <corners android:radius="60dip" />
    <padding
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp"
        />
</shape>